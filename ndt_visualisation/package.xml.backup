<package>
  <name>ndt_visualisation</name>
  <version>1.0.0</version>
  <description>Contains classes for creating an ndt visualization window. Initial implementation on top of mrpt.</description>
  <maintainer email="">Todor Stoyanov</maintainer>
  <maintainer email="">Jari Saarinen</maintainer>
  <maintainer email="">Henrik Andreasson</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/ndt_visualization</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Todor Stoyanov</author>
  <author>Jari Saarinen</author>
  <author>Henrik Andreasson</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>common_rosdeps</build_depend>
  <build_depend>pcl</build_depend>
  <build_depend>ndt_map</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>common_rosdeps</run_depend>
  <run_depend>pcl</run_depend>
  <run_depend>ndt_map</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>common_rosdeps</test_depend> -->
  <!-- <test_depend>pcl</test_depend> -->
  <!-- <test_depend>ndt_map</test_depend> -->

<export>
    <cpp lflags="-L${prefix}/lib -lndt_visualisation " cflags="-I${prefix}/include"/>
</export>

</package>